<% provide(:title, "Meeting") %>

<h3><%= @meeting.title %></h3>

<h4><%= @meeting.time.strftime("%m/%d/%Y") %></h4>

<h4>Materials</h4>
<ul>
  <% @meeting.meeting_materials.each do |meeting_material| %>
    <li><%= link_to meeting_material.material.file_name, 
                    meeting_material.material.url %>
    </li>
  <% end %>
</ul>